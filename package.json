    "body-parser": "^1.20.0",
    "celebrate": "^15.0.0",
    "cloudinary": "^1.28.1",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "csurf": "^1.11.0",
    "dotenv": "^10.0.0",
    "easyimage": "^3.1.1",
    "email-templates": "^9.0.0",
    "events": "^3.3.0",
    "express": "^4.17.2",
    "express-jwt": "^6.1.0",
    "express-validator": "^6.14.0",
    "firebase": "^9.6.8",
    "form-data": "^4.0.0",
    "formidable": "^2.0.1",
    "gm": "^1.23.1",
    "googleapis": "^96.0.0",
    "gravatar": "^1.8.2",
    "hashids": "^2.2.10",
