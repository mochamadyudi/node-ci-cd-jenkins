{
  "name": "node-ci-cd-jenkins",
  "version": "1.0.0",
  "description": "Backend application for skripsi",
  "main": "app.js",
  "proxy": "http://localhost:3000",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon --exec babel-node app.js",
    "production":"babel-node app.js",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "cron": "nodemon --exec babel-node src/loaders/jobs.js",
    "start:prod": "npm run build && node ./build/index.js",
    "start:migrate": "node ./album-migrations/index.js",
    "start:dev": "nodemon --exec babel-node app.js",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install && npm install babel-cli -g -S",
    "seeder:import": "babel-node config/seeder.js",
    "seeder:destroy": "babel-node config/seeder.js --d"
  },
  "keywords": [
    "skripsi",
    "pariwisata",
    "travel",
    "app",
    "travel"
  ],
  "author": "mochamad yudi sobari",
  "license": "MIT",
